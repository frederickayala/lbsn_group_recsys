[settings]
mapbox_access_token=
categories_file=/home/my_user/lbsn_group_recsys/additional_data/category_hierarchy_all.csv

[mexico_city]
#Dataset settings
dataset_folder=/home/my_user/lbsn_group_recsys/foursquare_data/parsed_dataset/experiment_top3/clean_dataset
experiments_folder=/home/my_user/lbsn_group_recsys/foursquare_data/parsed_dataset/experiment_top3/mexico_city_geo_optimized/
distance_to_group_folder=/home/my_user/lbsn_group_recsys/foursquare_data/parsed_dataset/experiment_top3/distance_to_group/
fixed_geonameid=3530597

#Splitter configurations
run_splits=split_by_cluster
item_test_proportion=.3
max_test_users_percent=.3
#Baselines configurations. For all the baselines use: item_consumption_similarity,item_consumption_content_similarity,implicit_matrix_factorization,geosoca
run_baselines=item_to_item,content_based_recommender_geo,content_based_recommender_cat,content_based_recommender_catgeo,implicit_matrix_factorization,sgd_matrix_factorization,sgd_content_matrix_factorization_geo,sgd_content_matrix_factorization_cat,sgd_content_matrix_factorization_catgeo,popularity_recommender,popularity_recommender_geo,popularity_recommender_cat,popularity_recommender_catgeo
run_experiment_models=projected_kde
kernel=gaussian
bandwidth=0.2
evaluation_cutoffs=5,10,20,30,40,50
num_factors=50
#Combine recommendations
run_combinations=average,average_no_misery,least_misery

